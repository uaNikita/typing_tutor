{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "default-case": "off",
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "__v"
        ]
      }
    ],
    "import/extensions": "off",
    "import/no-cycle": "off",
    "react/prop-types": "off"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "globals": {
    "BROWSER": true
  }
}